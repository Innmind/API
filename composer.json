{
    "name": "innmind/library",
    "license": "MIT",
    "description": "Holds crawled page data",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Domain\\": "src/Domain/",
            "App\\": "src/App/",
            "Web\\": "src/Web/",
            "CLI\\": "src/CLI/"
        },
        "files": [
            "src/App/bootstrap.php",
            "src/Domain/bootstrap.php",
            "src/Web/bootstrap.php",
            "src/CLI/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Domain\\": "tests/Domain"
        }
    },
    "require": {
        "php": "~7.2",
        "innmind/url": "~2.0",
        "innmind/immutable": "~2.0",
        "innmind/event-bus": "~3.0",
        "innmind/time-continuum": "^1.0",
        "jeremykendall/php-domain-parser": "~5.0",
        "innmind/specification": "~2.0",
        "innmind/colour": "~2.0",
        "innmind/command-bus": "~3.0",
        "innmind/rest-server": "~7.0",
        "innmind/neo4j-onm": "~6.0",
        "innmind/http-framework": "^1.2",
        "innmind/http-server": "^1.4",
        "innmind/logger": "^1.0",
        "innmind/http-authentication": "^1.4",
        "innmind/installation-monitor": "~2.0",
        "innmind/cli": "^1.3"
    },
    "config": {
        "bin-dir": "bin"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "infection/infection": "^0.7.1",
        "symfony/var-dumper": "^4.1",
        "innmind/debug": "^1.0"
    },
    "scripts": {
        "test" : "bin/phpunit --colors=always"
    },
    "bin": ["bin/library"]
}
