{
    "name": "innmind/library",
    "license": "MIT",
    "description": "Holds crawled page data",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Domain\\": "src/Domain/",
            "App\\": "src/App/",
            "Web\\": "src/Web/",
            "CLI\\": "src/CLI/"
        },
        "files": [
            "src/App/bootstrap.php",
            "src/Domain/bootstrap.php",
            "src/Web/bootstrap.php",
            "src/CLI/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Domain\\": "tests/Domain"
        }
    },
    "require": {
        "php": "~7.4",
        "innmind/url": "~3.3",
        "innmind/immutable": "~3.5",
        "innmind/event-bus": "~4.0",
        "innmind/time-continuum": "~2.2",
        "jeremykendall/php-domain-parser": "~5.0",
        "innmind/specification": "~2.0",
        "innmind/colour": "~3.1",
        "innmind/command-bus": "~4.0",
        "innmind/rest-server": "~8.0",
        "innmind/neo4j-onm": "~7.0",
        "innmind/http-framework": "~2.0",
        "innmind/http-server": "~2.0",
        "innmind/logger": "~2.0",
        "innmind/http-authentication": "~2.0",
        "innmind/installation-monitor": "~3.0",
        "innmind/cli": "~2.0",
        "innmind/stack": "^1.0",
        "innmind/silent-cartographer": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "infection/infection": "^0.15",
        "symfony/var-dumper": "^4.1",
        "innmind/debug": "~2.0",
        "vimeo/psalm": "^3.10"
    },
    "bin": ["bin/library"],
    "extra": {
        "gene": {
            "expression": ["bin/library install"]
        }
    }
}
